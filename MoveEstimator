using System;
class Program {
    static void Main(string[] args) {
        const double baseRate = 200.0;
        const double hourlyRate = 150.0;
        const double mileageRate = 2.0;
        Console.Write("Enter the estimated number of hours: ");
        double hours = Convert.ToDouble(Console.ReadLine());
        Console.Write("Enter the estimated number of miles: ");
        double miles = Convert.ToDouble(Console.ReadLine());
        double totalFee = baseRate + (hourlyRate * hours) + (mileageRate * miles);
        Console.WriteLine($"The total moving fee is: ${totalFee}");
    }
}
